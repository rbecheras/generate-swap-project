{
  "name": "generate-swap-project",
  "description": "A generator for SWAP projects",
  "version": "0.1.0",
  "homepage": "https://github.com/rbecheras/generate-swap-project",
  "author": "RÃ©mi Becheras (https://github.com/rbecheras)",
  "repository": "rbecheras/generate-swap-project",
  "bugs": {
    "url": "https://github.com/rbecheras/generate-swap-project/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clear": "yarn rimraf build dist",
    "lint": "yarn lint:esnext",
    "lint:esnext": "yarn standard src/*.js src/**/*.js",
    "build": "yarn build:lib && yarn build:tests && yarn build:docs",
    "build:lib": "yarn babel src/lib/ --out-dir dist/lib --source-maps",
    "build:tests": "yarn babel src/tests/ --out-dir dist/tests --source-maps",
    "build:docs": "echo WARNING: No documentation build available yet",
    "test": "yarn mocha dist/tests/**/*.js",
    "pipeline": "yarn clear && yarn lint && yarn build && yarn test",
    "travis": "yarn pipeline"
  },
  "keywords": [
    "generate",
    "generategenerator",
    "project",
    "swap",
    "nodejs",
    "javascript"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "delete": "^1.1.0",
    "generate": "^0.14.0",
    "generate-dest": "^0.1.4",
    "mocha": "^4.0.1",
    "npm-install-global": "^1.0.0",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "debug": "^3.1.0",
    "extend": "^3.0.1",
    "prettier-inspect": "^1.0.0"
  },
  "babel": {
    "presets": [
      "stage-0",
      [
        "env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false,
          "regenerator": true,
          "moduleName": "babel-runtime"
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "build",
      "dist",
      "node_modules/**"
    ],
    "env": {
      "mocha": true
    }
  }
}
