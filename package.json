{
  "name": "generate-swap-project",
  "description": "A generator for SWAP projects",
  "version": "0.1.0",
  "homepage": "https://github.com/rbecheras/generate-swap-project",
  "author": "RÃ©mi Becheras (https://github.com/rbecheras)",
  "repository": "rbecheras/generate-swap-project",
  "bugs": {
    "url": "https://github.com/rbecheras/generate-swap-project/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "lint": "yarn run lint:standard",
    "lint:standard": "yarn run standard generator.js index.js src/*.js src/**/*.js",
    "test": "mocha"
  },
  "keywords": [
    "generate",
    "generategenerator",
    "project",
    "swap",
    "nodejs",
    "javascript"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "delete": "^1.1.0",
    "generate": "^0.14.0",
    "mocha": "^4.0.1",
    "npm-install-global": "^1.0.0",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "extend": "^3.0.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false,
          "regenerator": true,
          "moduleName": "babel-runtime"
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "build",
      "dist",
      "node_modules/**"
    ],
    "env": {
      "mocha": true
    }
  }
}
